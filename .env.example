# USDA Food Data Central API key
# Get your API key from https://fdc.nal.usda.gov/api-key-signup.html
FDC_API_KEY=your_api_key_here

# API URL (usually doesn't need to be changed)
FDC_API_URL=https://api.nal.usda.gov/fdc/v1

# Django cache settings
FDC_CACHE_ENABLED=True
FDC_CACHE_TIMEOUT=86400  # 24 hours in seconds